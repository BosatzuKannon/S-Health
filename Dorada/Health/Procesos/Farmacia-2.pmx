        <dyn_content><![CDATA[{"name":"FRM_FORMULA_REVISION","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"2857071125e6a7b8d4eed57073001351","name":"FRM_FORMULA_REVISION","description":"","mode":"edit","script":{"type":"js","code":"inicio_formula_revision();\n\nfunction inicio_formula_revision(){    \n  \n  $(\"#button2001\").hide();\n  $(\"#gridVar2001\").hide();\n  $(\"#dialogPanel1\").hide();\n  $(\"#suggestVar1003\").setOnchange(fr_1);  \n}\n\nfunction fr_1(){\n\n  if($(\"#suggestVar1003\").getValue() == \"\") {\n\n      $(\"#button2001\").hide();      \n    }\n    else{\n\n      $(\"#button2001\").show();\n    }\n}\n\n$(\"#button2001\").find(\"button\").on(\"click\" , function(){\n\n  if($(\"#suggestVar1003\").getValue() == ''){    \n    return false;\n  }  \n    \n  var q = $(\"#suggestVar1003\").getValue();  \n  var rows;    \n    \n  if (window.XMLHttpRequest) // code for IE7+, Firefox, Chrome, Opera, Safari\n      xmlhttp=new XMLHttpRequest();\n  else // code for IE5, IE6\n      xmlhttp=new ActiveXObject(\"Microsoft.XMLHTTP\");\n \n  xmlhttp.onreadystatechange=function(){\n      if(xmlhttp.readyState==4 && xmlhttp.status==200){        \n       \n        var rows = eval('(' + xmlhttp.responseText + ')');\n        \n        if(rows.length == 0){\n         \n          showConfirmDlg();\n          return false;\n        }\n        else{\n          \n          $(\"#gridVar2001\").show();\n          console.log(\"\"+rows.length);\n          for (var i = 0; i < rows.length; i++){\n\n            if(i == 0){\n              $(\"#gridVar2001\").addRow();\n              $(\"#gridVar2001\").setText(rows[i].fecha,1, 1);\n              $(\"#gridVar2001\").setText(rows[i].servicio,1, 2);\n              $(\"#gridVar2001\").setText(rows[i].medico,1, 3);\n              $(\"#gridVar2001\").setText(rows[i].caso,1, 5);\n              $(\"#gridVar2001\").setText(rows[i].observacion,1, 6);\n            }\n            else{\n              var aData = [\n                  {value: rows[i].fecha},\n                  {value: rows[i].servicio},\n                  {value: rows[i].medico},\n                  {value: \"\"},\n                  {value: rows[i].caso},\n                  {value: rows[i].observacion}\n              ];\n              $(\"#gridVar2001\").addRow(aData);\n            }            \n          }\n        }\n      }\n   }\n   xmlhttp.open(\"GET\", \"/hlt_buscar_formulas.php?q=\"+q, true);\n   xmlhttp.send();\n});\n\nfunction showConfirmDlg(){\n    var dlgContents = {\n        modal:     true,\n        resizable: false,\n        buttons: {\n            \"Continuar\": function() {\n                $(this).dialog(\"close\");\n                return false;\n            }\n        }\n    }\n    $(\"#newClientDialog\").dialog(dlgContents);\n}"},"language":"en","externalLibs":"http://code.jquery.com/ui/1.11.4/jquery-ui.js,\nhttp://code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css","printable":false,"items":[[{"type":"title","id":"title2001","label":"FARMACIA - REVISIÓN DE FORMULAS","colSpan":12}],[{"type":"form","variable":"","var_uid":"","dataType":"","id":"6708531565e6a7b8d4ecc43099058405","name":"FRM_INFORMACION_PACIENTE","description":"","mode":"edit","script":{"type":"js","code":"$(\"#button1001\").find(\"button\").on(\"click\" , function(){\n\t\n\twindow.open(\"http://orangecol.tk/sysamericana/es-ES/neoclassic/3196788185e5db675cac2c1081861327/2237636135d44bad1895dc1092946238.php\", \"\", \"width=1200,height=600\");\t\n});"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"subtitle","id":"subtitle1001","label":"INFORMACIÓN DEL PACIENTE","colSpan":12}],[{"type":"suggest","variable":"suggestVar1003","var_uid":"3899097295e73edd184d413029917316","dataType":"string","protectedValue":false,"id":"suggestVar1003","name":"suggestVar1003","label":"Documento","defaultValue":"","placeholder":"","hint":"","required":false,"requiredFieldErrorMessage":"","mode":"parent","dat