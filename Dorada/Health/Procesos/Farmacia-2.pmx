      <record>
        <tri_uid>3489084655e740c8bd00115060723542</tri_uid>
        <tri_title>TGG_CARGAR_MEDICAMENTOS</tri_title>
        <tri_description></tri_description>
        <pro_uid>1129324295e6a7b8bab0907086439698</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[foreach (@=gridVar2001 as $aRow) {
	
	if($aRow['checkbox2001'] == '1'){

		$caso = $aRow['hidden2001'];
		$observacion = $aRow['hidden2002'];
		
	}
}

@@CASO_CARAJO = $caso;
@@OBSERVACION = $observacion;

@@query1=$query = "select b.cod_medicamento,b.descripcion,b.forma,b.concentracion,a.cantidad,a.dosis,a.consecutivo_formula_medica from hlt_formula_medica a left join hlt_medicamento b on a.consecutivo_medicamento = b.consecutivo_medicamento where a.caso = '$caso';";
$res = executeQuery($query,'8882909715d5ac6f58ca441019038204');

if (is_array($res) and count($res) > 0){
    @=gridVar3001 = $res;
}
//carga enfermeda y tratamiento
@@quer2=$query = "select enfermedad_actual, tratamiento from hlt_consulta_externa where caso ='$caso'";


$result = executeQuery($query,'8882909715d5ac6f58ca441019038204');

@@enfermedad_actual =  $result['1']['enfermedad_actual'];
@@tratamiento = $result['1']['tratamiento'];

@@query3=$query = "select cod_profesional, profesional from hlt_citas where caso = '$caso'";

$result = executeQuery($query,'8882909715d5ac6f58ca441019038204');

@@cod_profesional =  $result['1']['cod_profesional'];
@@profesional =  $result['1']['profesional'];

//firmas medico
$usr_uid = @@cod_profesional;
@@firma_medico = "<img src=\""."./firmas/".$usr_uid.".png\" alt=\"firma\" height=\"50\" width=\"100\">";

@@sello_medico = "<img src=\""."http://health.ipsdorada.com/sellos/".$usr_uid.".png\" alt=\"sello\" height=\"50\" width=\"100\">";

//firma paciente
$usr_uid_paciente = @@suggestVar1003;
@@firma_paciente = "<img src=\""."./firmas_pacientes/".$usr_uid_paciente.".png\" alt=\"firma\" height=\"50\" width=\"100\">";]]></tri_webbot>
        <tri_param></tri_param>
      </record>
      <record>
        <tri_uid>4750873165ee3de54146cc1060219054</tri_uid>
        <tri_title>TGG_FACTURAR_PRE</tri_title>
        <tri_description></tri_description>
        <pro_uid>1129324295e6a7b8bab0907086439698</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$no_historia = @@suggestVar1003;
@@fecha_entrega = $fecha = date("Y-m-d H:i:s", strtotime("-5 hours"));

$query = "select calificacion from hlt_pacientes where no_historia='$no_historia';";
$result = executeQuery($query,'8882909715d5ac6f58ca441019038204');
$calificacion =  $result['1']['calificacion'];

$regimen = @@textVar3017;
$cuota = "0";

if($regimen == "Particular"){
	$cuota = "3400";
  @@textVar600136 = $cuota; 
}
else{

  if($regimen == "Contributivo"){

    if($calificacion == "1"){
      $cuota = "3400";
      @@textVar600136 = $cuota;     
    }
    else if($calificacion == "2"){
      $cuota = "13500";
      @@textVar600136 = $cuota;     
    }
    else{
      $cuota = "35600";
      @@textVar600136 = $cuota;     
    }
  }
  else{
    $cuota = "0";
    @@textVar600136 = $cuota;   
  }
}]]></tri_webbot>
