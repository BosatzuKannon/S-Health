        <tri_title>TGG_ENTREGA_MEDICAMENTOS</tri_title>
        <tri_description></tri_description>
        <pro_uid>1129324295e6a7b8bab0907086439698</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[$no_historia = @@suggestVar1003;
@@FECHA_IMP = $fecha = date("Y-m-d H:i:s", strtotime("-5 hours"));

foreach (@=gridVar3001 as $aRow) {
	
	if($aRow['checkbox0000000001'] == '1'){

		$estado = '1';
		$consecutivo_formula_medica = @@consecutivo_formula_medica;
		$query 	= "update hlt_formula_medica set estado = '$estado', fecha_entrega='$fecha' where no_historia='$no_historia'; ";
 		$result = executeQuery($query,'8882909715d5ac6f58ca441019038204');
	}
}]]></tri_webbot>
